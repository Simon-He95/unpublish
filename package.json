{
  "name": "@simon_he/unpublish",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@8.15.9",
  "description": "",
  "author": "Simon He",
  "license": "MIT",
  "funding": "https://github.com/sponsors/Simon-He95",
  "homepage": "https://github.com/Simon-He95/pkg-name#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Simon-He95/pkg-name.git"
  },
  "bugs": "https://github.com/Simon-He95/pkg-name/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "bin": {
    "unpublish": "./cli.mjs"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup ./src/index.ts --format cjs,esm --dts --clean --minify",
    "dev": "npm run build -- --watch src",
    "format": "prettier --write --cache .",
    "lint": "eslint . --ext .vue,.js,.ts,.jsx,.tsx,.md,.json --max-warnings 0 --cache",
    "lint:fix": "pnpm run lint --fix",
    "prepublishOnly": "nr build",
    "release": "bumpp && npm publish",
    "start": "esno src/index.ts",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@simon_he/colorize": "^0.0.1",
    "lazy-js-utils": "^0.1.27"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.34.2",
    "@types/node": "^18.19.67",
    "bumpp": "^8.2.1",
    "eslint": "^8.57.1",
    "esno": "^0.17.0",
    "lint-staged": "^13.3.0",
    "picocolors": "^1.1.1",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "tsup": "^6.7.0",
    "tsx": "^3.14.0",
    "typescript": "^4.9.5",
    "vitest": "^0.26.3"
  },
  "lint-staged": {
    "*": [
      "prettier --write --cache --ignore-unknown"
    ],
    "*.{vue,js,ts,jsx,tsx,md,json}": "eslint --fix"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
